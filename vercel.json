{
  "version": 2,
  "buildCommand": "echo 'Static site - no build required'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/vet-project",
      "dest": "/vet-project/index.html"
    },
    {
      "src": "/vet-project/(.*)",
      "dest": "/vet-project/$1"
    }
  ]
}
