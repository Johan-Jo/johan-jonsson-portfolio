{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "vet-project/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/vet-project",
      "dest": "/vet-project/index.html"
    },
    {
      "src": "/vet-project/api/(.*)",
      "dest": "/vet-project/server.js"
    }
  ]
}
